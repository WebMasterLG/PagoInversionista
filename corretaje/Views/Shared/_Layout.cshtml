    string Area = Request.Cookies["cookiePerfil"]["idArea"].ToString();
    string Perfil = Request.Cookies["cookiePerfil"]["perfil"].ToString();

    var usuconect = UsuariosModels.Trae_Datos_Usuario_Conectado(int.Parse(idUsuario));
}
