    @RenderSection("HeaderJS", required: false)
    <link href="~/img/Logo_if_final.ico" type="image/x-icon" rel="shortcut icon" />

    <style>
        /*        body {
            color: #fff;
        }*/
        .pointer {
            cursor: pointer;
        }

        .text-small {
            font-size: 15px;
        }

        .lista {
            width: 180px;
        }

        .cont_input {
            width: 75px;
            margin-left: 9px;
            border: 0px solid #fff;
        }

        #tabla td {
            font-size: 12px;
            text-align: center;
            padding: 2px;
            border-top: 1px solid #fff;
            border-left: 1px solid #fff;
        }

        #tit td {
            font-size: 11px;
            text-align: center;
            vertical-align: top;
            font-family: OpenSans-Bold;
        }

        .label {
            color: #ff8000;
            font-size: 10px;
        }

        .label2 {
            color: #ff8000;
            font-size: 12px;
            padding-left: 10px;
        }

        .opc_home {
            font-size: 14px;
        }

        .boton:hover {
            background-color: #ccc;
            color: #000;
            font-weight: bolder;
        }

        .modal3 {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            background-color: #fff;
            margin: 4rem auto;
            height: calc(100vh - 8rem);
            box-shadow: 0 0px 10px rgba(0,0,0,0.6);
            z-index: 1002 !important;
            border-radius: 5px 5px 5px 5px;
            -moz-border-radius: 5px 5px 5px 5px;
            /*para Safari y Chrome*/
            -webkit-border-radius: 5px 5px 5px 5px;
        }

        .overlay2 {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: #000;
            z-index: 1001;
            opacity: .6;
            -moz-opacity: .6;
            filter: alpha(opacity=60);
        }

        body.modal-wrapper {
            position: fixed !important;
            overflow: hidden;
        }

        .icon {
            font-size: 1.5rem;
            line-height: 1.5rem;
            vertical-align: middle;
        }

        .orange {
            color: #FF7219;
        }

        .red {
            color: red;
        }

        .green {
            color: forestgreen;
        }

        .yellow {
            color: yellow;
        }

        .loader-overlay {
            z-index: 9999;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: transparent;
        }

        tags {
            width: 100%
        }

        /*!
         * loadme v1.0.0 (https://github.com/zx1988826/loadme)
         * Copyright 2017 Tencent, Inc.
         * Licensed under the MIT license
         */
        .loadme-mask {
            background-color: #ddd;
            width: 100px;
            height: 100px;
            position: fixed;
            z-index: 99;
            left: 50%;
            top: 50%;
            margin: -50px 0 0 -50px;
            border-radius: 10px;
            opacity: 0.5;
        }

        .loadme-circular,
        .loadme-circular:after {
            border-radius: 50%;
            width: 10em;
            height: 10em;
        }

        .loadme-circular {
            width: 40px;
            height: 40px;
            position: fixed;
            left: 50%;
            top: 50%;
            margin: -20px 0 0 -20px;
            z-index: 100;
            font-size: 10px;
            text-indent: -9999em;
            border-top: 1px solid rgba(237, 237, 237, 0.8);
            border-right: 1px solid rgba(237, 237, 237, 0.8);
            border-bottom: 1px solid rgba(237, 237, 237, 0.8);
            border-left: 1px solid #000;
            -webkit-transform: translateZ(0);
            transform: translateZ(0);
            -webkit-animation: loadme-circular-animate 1.1s infinite linear;
            animation: loadme-circular-animate 1.1s infinite linear;
        }

        @@-webkit-keyframes loadme-circular-animate {
            0% {
                -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
            }

            100% {
                -webkit-transform: rotate(360deg);
                transform: rotate(360deg);
            }
        }

        @@keyframes loadme-circular-animate {
            0% {
                -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
            }

            100% {
                -webkit-transform: rotate(360deg);
                transform: rotate(360deg);
            }
        }

        .windy-flag {
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .loading-text {
            position: fixed;
            top: 60%;
            left: 45%;
        }

            .loading-text span {
                display: inline-block;
                font-weight: bold;
                font-size: 20px;
                color: coral;
                margin: 0 5px;
                transform-origin: bottom center;
                animation: waveAnimation 2s ease infinite;
            }

                .loading-text span:nth-child(1) {
                    animation-delay: 0.1s;
                }

                .loading-text span:nth-child(2) {
                    animation-delay: 0.2s;
                }

                .loading-text span:nth-child(3) {
                    animation-delay: 0.3s;
                }

                .loading-text span:nth-child(4) {
                    animation-delay: 0.4s;
                }

                .loading-text span:nth-child(5) {
                    animation-delay: 0.5s;
                }

                .loading-text span:nth-child(6) {
                    animation-delay: 0.6s;
                }

                .loading-text span:nth-child(7) {
                    animation-delay: 0.7s;
                }

                .loading-text span:nth-child(8) {
                    animation-delay: 0.8s;
                }

                .loading-text span:nth-child(9) {
                    animation-delay: 0.9s;
                }

                .loading-text span:nth-child(10) {
                    animation-delay: 1.0s;
                }

                .loading-text span:nth-child(11) {
                    animation-delay: 1.1s;
                }

                .loading-text span:nth-child(12) {
                    animation-delay: 1.2s;
                }

        @@keyframes waveAnimation {
            0%, 100% {
                transform: translateY(0);
            }

            50% {
                transform: translateY(-15px);
            }
        }

        #NewModal2024 .container {
            width: 100% !important;
            background: linear-gradient(180deg, #f09819 0%, #ff5858 100%) !important;
            padding: 1rem;
            border-radius: 20px;
            display: flex !important;
        }

        #NewModal2024 .navbar {
            align-items: center;
            /*justify-content: space-between;*/
            gap: 1rem;
            margin-bottom: 0px !important;
        }

        #NewModal2024 .list {
            gap: 2rem !important;
            border-radius: 20px !important;
            display: flex;
            flex-wrap: wrap;
            list-style: none;
        }

        #NewModal2024 i {
            font-size: 30px;
            transition: opacity 0.3s ease-in-out;
        }

            #NewModal2024 i:hover {
                opacity: 0.5;
                cursor: pointer;
            }

        #NewModal2024 .list-item {
            padding: 1rem;
            border: 2px solid white;
            background-image: linear-gradient(to bottom, #FF8000, #FFA042);
            border-radius: 10px;
            color: #fff;
            padding: .5rem;
            position: relative;
            white-space: nowrap;
        }

            #NewModal2024 .list-item::after {
                content: " ";
                position: absolute;
                bottom: 0;
                left: 0;
                width: 100%;
                height: 2px;
                background: #fff;
                border-radius: 5px;
                transform: scaleX(0);
                transition: all 0.6s ease;
            }

            #NewModal2024 .list-item:hover::after {
                transform: scaleX(1);
            }

        #NewModal2024 li:not(.list-item.active) a {
            color: white;
            font-weight: bold;
        }

        @@keyframes border-pulse {
            0% {
                box-shadow: 0 0 0 0 rgba(255, 69, 0, 0.7);
            }

            70% {
                box-shadow: 0 0 0 15px rgba(255, 69, 0, 0);
            }

            100% {
                box-shadow: 0 0 0 0 rgba(255, 69, 0, 0);
            }
        }

        #NewModal2024 .list-item.active {
            background: #6C757D !important;
            animation: border-pulse 2s infinite; /* La animación durará 2 segundos y se repetirá infinitamente */
        }

            #NewModal2024 .list-item.active a {
                color: orange !important;
                font-weight: bold;
            }


        [tooltip] {
            position: relative;
        }

            [tooltip]:hover::before {
                content: attr(tooltip);
                position: absolute;
                bottom: -30px;
                display: inline-block;
                padding: 3px 10px;
                border-radius: 5px;
                background: #000;
                color: #fff;
                font-size: 12px;
                font-family: sans-serif;
                white-space: nowrap;
                z-index: 9999;
            }

            [tooltip]:hover::after {
                content: '';
                position: absolute;
                bottom: -10px;
                left: 15px;
                display: inline-block;
                color: #fff;
                border: 8px solid transparent;
                border-bottom: 8px solid #000;
                z-index: 9999;
            }
    </style>

</head>
